<Page x:Class="Frontend.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Frontend"
        xmlns:costum ="clr-namespace:Frontend.View"
        mc:Ignorable="d"
         Height="450" Width="800" >

    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition Height="1*" />
            <RowDefinition Height="1.5*" />
            <RowDefinition Height="1*" />
        </Grid.RowDefinitions>

        <costum:CharacterView Grid.Row="0" Statistics="{Binding HH}"/>
        <!--<Image Source="{Binding CharacterImages.CharacterImage}" HorizontalAlignment="Left" Height="300" Margin="120,41.2,0,0" Grid.Row="1" VerticalAlignment="Top" Width="109"/>-->
    </Grid>
</Page>
